{
    "name": "yiisoft/log-target-email",
    "type": "library",
    "description": "Yii Logging Library - Email Target",
    "keywords": [
        "yii",
        "framework",
        "log",
        "logger"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/log-target-email",
        "issues": "https://github.com/yiisoft/log-target-email/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/log": "^3.0@dev",
        "yiisoft/mailer": "^3.0@dev"
    },
    "require-dev": {
        "yiisoft/di": "^3.0@dev",
        "yiisoft/event-dispatcher": "^3.0@dev",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Log\\Target\\Email\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Log\\Target\\Email\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
